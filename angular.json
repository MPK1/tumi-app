  "cli": {
    "analytics": "8ea22110-bc58-4100-8592-7bed9e3111ba",
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "schematics": {
    "@schematics/angular": {
      "component": {
        "changeDetection": "OnPush",
        "style": "scss"
      }
    },
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
