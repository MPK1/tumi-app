    "tumi-app": {
      "projectType": "application",
      "root": "apps/tumi-app",
      "sourceRoot": "apps/tumi-app/src",
      "prefix": "tumi",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/tumi-app/browser",
            "index": "apps/tumi-app/src/index.html",
            "main": "apps/tumi-app/src/main.ts",
            "polyfills": "apps/tumi-app/src/polyfills.ts",
            "tsConfig": "apps/tumi-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/tumi-app/src/favicon.ico",
              "apps/tumi-app/src/assets"
            ],
            "styles": ["apps/tumi-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/tumi-app/src/environments/environment.ts",
                  "with": "apps/tumi-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "tumi-app:build:production"
            },
            "development": {
              "browserTarget": "tumi-app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/tumi-app/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tumi-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tumi-app/src/**/*.ts",
              "apps/tumi-app/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tumi-app"],
          "options": {
            "jestConfig": "apps/tumi-app/jest.config.js",
            "passWithNoTests": true
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/apps/tumi-app/server",
            "main": "apps/tumi-app/server.ts",
            "tsConfig": "apps/tumi-app/tsconfig.server.json",
            "inlineStyleLanguage": "scss",
            "externalDependencies": [
              "firebase",
              "@firebase/app",
              "@firebase/analytics",
              "@firebase/auth",
              "@firebase/component",
              "@firebase/database",
              "@firebase/firestore",
              "@firebase/functions",
              "@firebase/installations",
              "@firebase/messaging",
              "@firebase/storage",
              "@firebase/performance",
              "@firebase/remote-config",
              "@firebase/util"
            ]
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/tumi-app/src/environments/environment.ts",
                  "with": "apps/tumi-app/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "tumi-app:build:development",
              "serverTarget": "tumi-app:server:development"
            },
            "production": {
              "browserTarget": "tumi-app:build:production",
              "serverTarget": "tumi-app:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "tumi-app:build:production",
              "serverTarget": "tumi-app:server:production"
            },
            "development": {
              "browserTarget": "tumi-app:build:development",
              "serverTarget": "tumi-app:server:development"
            }
          },
          "defaultConfiguration": "production"
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "ssr": true
          }
        }
      }
    },
    "tumi-app-e2e": {
      "root": "apps/tumi-app-e2e",
      "sourceRoot": "apps/tumi-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tumi-app-e2e/cypress.json",
            "tsConfig": "apps/tumi-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "tumi-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tumi-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/tumi-app-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      }
    },
    "tumi-functions": {
      "root": "apps/tumi-functions",
      "sourceRoot": "apps/tumi-functions/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@simondotm/nx-firebase:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/tumi-functions",
            "main": "apps/tumi-functions/src/index.ts",
            "tsConfig": "apps/tumi-functions/tsconfig.app.json",
            "packageJson": "apps/tumi-functions/package.json",
            "assets": [
              "apps/tumi-functions/*.md",
              "apps/tumi-functions/.runtimeconfig.json"
            ]
          }
        },
        "deploy": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "firebase deploy --config firebase.tumi-functions.json"
          }
        },
        "getconfig": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "firebase functions:config:get --config firebase.tumi-functions.json > apps/tumi-functions/.runtimeconfig.json"
          }
        },
        "emulate": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "firebase emulators:start --config firebase.tumi-functions.json"
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "nx run tumi-functions:build --with-deps && nx run tumi-functions:build --watch"
              },
              {
                "command": "nx run tumi-functions:emulate"
              }
            ],
            "parallel": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/tumi-functions/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tumi-functions"],
          "options": {
            "jestConfig": "apps/tumi-functions/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
