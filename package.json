{
  "name": "tumi-app",
  "version": "2.1.0-deploy.33",
  "private": true,
  "scripts": {
    "build:all": "yarn run build:ssr && yarn run copy:hosting",
    "build:functions": "npm run --prefix functions build",
    "build:prod": "ng build --configuration production",
    "build:ssr": "ng build --configuration production --source-map && ng run tumi-app:server:production",
    "copy:hosting": "copyfiles dist/**/* functions",
    "deploy": "ng deploy",
    "deploy:all": "firebase deploy",
    "deploy:functions": "yarn run build:all && pm run --prefix functions deploy",
    "dev:ssr": "ng run tumi-app:serve-ssr",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "postdeploy:all": "cross-var sentry-cli releases files \"tumi-app@$npm_package_version\" upload-sourcemaps ./dist/tumi-app/browser && cross-var sentry-cli releases deploys \"tumi-app@$npm_package_version\" new -e production && cross-var sentry-cli releases finalize \"tumi-app@$npm_package_version\"&& cross-var sentry-cli releases deploys \"universal-render@$npm_package_version\" new -e production && cross-var sentry-cli releases finalize \"universal-render@$npm_package_version\"",
    "postrelease:deploy": "yarn run sentry:release:start",
    "prebuild:all": "yarn run release:deploy",
    "precopy:hosting": "rimraf functions/dist",
    "predeploy:all": "yarn run build:all",
    "prerender": "ng run tumi-app:prerender",
    "release": "standard-version --no-verify",
    "release:deploy": "standard-version --prerelease deploy --no-verify --skip.changelog --skip.tag --skip.commit",
    "release:preview": "standard-version --no-verify --dry-run",
    "sentry:release:start": "cross-var sentry-cli releases new \"tumi-app@$npm_package_version\" && cross-var sentry-cli releases new \"universal-render@$npm_package_version\"",
    "serve:ssr": "node dist/tumi-app/server/main.js",
    "start": "ng serve",
    "statistics": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/tumi-app/browser/stats.json",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "~12.1.3",
    "@angular/cdk": "^12.1.3",
    "@angular/common": "~12.1.3",
    "@angular/compiler": "~12.1.3",
    "@angular/core": "~12.1.3",
    "@angular/fire": "^6.1.5",
    "@angular/flex-layout": "^12.0.0-beta.34",
