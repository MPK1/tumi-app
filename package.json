  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && patch-package && yarn run generate:graphql",
    "nx": "nx",
    "start": "node dist/apps/api/main.js",
    "start:app": "nx serve tumi-app",
    "start:api": "nx serve api",
    "build:api": "nx build api",
    "test": "ng test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "ng run tumi-app:serve-ssr",
    "serve:ssr": "node dist/tumi-app/server/main.js",
    "build:ssr": "ng build && ng run tumi-app:server",
    "prerender": "ng run tumi-app:prerender",
    "generate:graphql": "patch-package && nx run server-models:generate && nx run data-access:generate",
    "db:push": "nx run server-models:push",
    "postdb:push": "yarn generate:graphql",
    "statistics": "nx build tumi-app --configuration production --stats-json && npx webpack-bundle-analyzer dist/apps/tumi-app/browser/stats.json"
  },
